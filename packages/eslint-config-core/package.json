{
  "name": "@pavl-ro/eslint-config-core",
  "version": "0.8.4",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.esm.d.ts",
  "license": "MIT",
  "author": "Pavel Romanov",
  "description": "Core ESLint configuration",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.esm.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "typescript",
    "javascript",
    "lint",
    "core"
  ],
  "dependencies": {
    "@eslint/js": "^9.25.1",
    "eslint-plugin-jsdoc": "^50.6.11",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.31.0"
  },
  "peerDependencies": {
    "eslint": "^9.25.1"
  }
}
